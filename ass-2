#include <stdio.h>
int main() 
{
    int n1, n2, i, count;
    printf("Enter n1 and n2: ");
    scanf("%d%d", &n1, &n2);
    count = 0;
    for (i = n1; i <= n2; i++)
    {
        if (i % 2 != 0)
        {  
            count++;
            if (count % 3 != 0)
            {
                printf("%d ", i);
            }
       }
     }
      printf("\n");
    return 0;
}
